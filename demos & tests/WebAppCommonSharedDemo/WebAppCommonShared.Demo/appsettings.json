{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsightsInstrumentationKey": "your-application-insights-key-here",
  "LoggingRetentions": {
    "ErrorRetentionsInDay": 60,
    "WarningRetentionsInDay": 30,
    "DefaultRetentionsInDay": 2,
    "LoggerRules": {
      "HomeSphere.CaptureService.Core.BackgroundTasks.DeletedCapturesCleanUpTask": "01:00:00"
    }
  },
  "EnableOrigins": "localhost:8080",
  "JwtTokenIssuer": "com.vampirecoder.test",
  "JwtExpireInMinutes": 21600,
  "JwtTokenSigningKey": "",
  "JwtTokenPrivateKey": "your-jwt-private-key-here"
}
