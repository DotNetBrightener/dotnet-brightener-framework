<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<PackageId>DotNetBrightener.Caching.Redis</PackageId>
		<Description>
			A library that provides mechanism for caching data using Redis cache
		</Description>
		<PackageDescription>
			$(Description)
		</PackageDescription>
	</PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<PackageReference Include="StackExchange.Redis" Version="2.6.96" />
		<PackageReference Include="DotNetBrightener.Caching.Abstractions" Version="$(Version)" />
	</ItemGroup>
</Project>
