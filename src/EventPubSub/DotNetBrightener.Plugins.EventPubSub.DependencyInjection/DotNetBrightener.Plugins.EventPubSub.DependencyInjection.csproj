<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<VersionPrefix>2022.10.2</VersionPrefix>
		<DNBLibraryVersion>2022.10.2</DNBLibraryVersion>
		<Platforms>AnyCPU;ARM64</Platforms>
	</PropertyGroup>
	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<ItemGroup Condition="$(Configuration)=='Debug'">
		<ProjectReference Include="..\DotNetBrightener.Plugins.EventPubSub\DotNetBrightener.Plugins.EventPubSub.csproj" />
	</ItemGroup>

	<ItemGroup Condition="$(Configuration)=='Release'">
		<PackageReference Include="DotNetBrightener.Plugins.EventPubSub" Version="$(DNBLibraryVersion)" />
	</ItemGroup>

	<Import Project="..\..\Shared\SharedExtensions.ServiceCollection\SharedExtensions.ServiceCollectionExtensions.projitems" Label="Shared" />

</Project>
