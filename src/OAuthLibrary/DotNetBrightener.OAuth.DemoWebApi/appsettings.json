{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OAuthSettings": {
    "ForceHttps": false,
    "EnabledOrigins": "*",
    "DefaultMobileRedirectUrl": "myapp://oauth/callback",
    "DefaultWebRedirectUrl": "http://localhost:5034"
  },
  "GoogleOAuthSettings": {
    "ClientId": "your-google-client-id.apps.googleusercontent.com",
    "ClientSecret": "your-google-client-secret"
  },
  "AppleOAuthSettings": {
    "ClientId": "com.yourcompany.yourapp",
    "MobileClientId": "com.yourcompany.yourapp.mobile",
    "PrivateKey": "-----BEGIN PRIVATE KEY-----\nYOUR_APPLE_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----",
    "TeamId": "YOUR_APPLE_TEAM_ID",
    "KeyId": "YOUR_APPLE_KEY_ID"
  }
}
