{
  "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
  "version": "2025.0.4",
  "assemblyVersion": {
    "precision": "build"
  },
  "nuGetPackageVersion": {
    "semVer": 2.0,
    "precision": "minor"
  },
  "publicReleaseRefSpec": [
    "^refs/heads/master$"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  },
  "release": {
    "branchName": "releases/v{version}",
    "firstUnstableTag": ""
  },
  "pathFilters": [
    "./"
  ]
}